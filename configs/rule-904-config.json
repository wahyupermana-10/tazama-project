{
  "id": "904@1.0.0",
  "cfg": "1.0.0",
  "desc": "Rapid Transaction Detection - Mendeteksi transaksi berulang dalam waktu singkat (lebih dari 1 transaksi dalam 1 menit)",
  "tenantId": "DEFAULT",
  "config": {
    "parameters": {
      "timeWindowMs": 60000,
      "description": "Time window dalam milliseconds (60000 = 1 menit)"
    },
    "bands": [
      {
        "subRuleRef": ".01",
        "lowerLimit": 0,
        "upperLimit": 2,
        "reason": "Normal - 1 transaksi dalam 1 menit"
      },
      {
        "subRuleRef": ".02",
        "lowerLimit": 2,
        "upperLimit": 4,
        "reason": "Suspicious - 2-3 transaksi dalam 1 menit"
      },
      {
        "subRuleRef": ".03",
        "lowerLimit": 4,
        "upperLimit": 6,
        "reason": "High Risk - 4-5 transaksi dalam 1 menit"
      },
      {
        "subRuleRef": ".04",
        "lowerLimit": 6,
        "upperLimit": 999999,
        "reason": "Critical - 6+ transaksi dalam 1 menit - INTERDICTION"
      }
    ],
    "exitConditions": [
      {
        "subRuleRef": ".x00",
        "reason": "Unsuccessful transaction"
      }
    ]
  }
}
